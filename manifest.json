{
    "name": "Easy HAR extractor",
    "version": "0.2",
    "manifest_version": 2,
    "minimum_chrome_version": "10.0",
    "description": "Easily extract the HAR network logs for diagnostic purposes",
    "icons": {
        "16": "/images/record-grey.png",
        "48": "/images/record-grey.png",
        "128": "/images/record-grey.png"
    },
    "devtools_page": "devtools.html",
    "permissions": [
        "http://*/*",
        "https://*/*",
        "tabs",
        "notifications",
        "downloads"
    ],
    "background": {
        "scripts": ["background.js"]
    }
}

